                      function AUTO22Quest8()
                  
                          local lp = game.Players.LocalPlayer
                                          
                              local function getNPC()
                          
                  
                              for i,v in pairs(game:GetService("Workspace").Scriptable.Mobs:GetChildren()) do
                                  if v.Name == "upperrankdemon" then
                                      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Head.CFrame
                                      
                                      
                                  end     
                              end
                          end
                  
                  
                          while getgenv().autoQuest22N == true do 
                              task.wait()
                                  pcall(function()
                  
                                      moveto(getNPC().HumanoidRootPart.CFrame + Vector3.new(50,100,300) ,100)
                                      wait()
                  
                                      
                                  
                                  end)
                                  loadstring(game:HttpGet("https://pastebin.com/raw/zAvxcTAi"))()
                                  wait()
                                  
                  
                                  local args = {
                                  [1] = "StartQuest",
                                  [2] = "BanditQuest8",
                                  [3] = 3
                                  }
                  
                                  game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Quests/RemoteEvent"):FireServer(unpack(args))
                          end
                      
                      end
                      
