 -- Tab10 
                    Tab10:AddLabel("Misc")
                    Tab10:AddLabel("credits: ebadgameryt")

                    Tab10:AddLabel("Join Discord Server for More Updates!")
                    Tab10:AddLabel("Discord Server Link: https://discord.gg/NJ3u2pVjcs")

                    Tab10:AddLabel("Scroll Down For Discord Server")



                    Tab10:AddToggle({
                        Name = "NoClip",
                        Default = false,
                        Callback = function(Value)
                            benoclip = Value
                            local Workspace = game:GetService("Workspace")
                            local Players = game:GetService("Players")
                            local Plr = Players.LocalPlayer
                            local Clipon = false


                            if benoclip == true then
                                Clipon = true
                                
                                Stepped = game:GetService("RunService").Stepped:Connect(function()
                                    if not Clipon == false then
                                        for a, b in pairs(Workspace:GetChildren()) do
                                        if b.Name == Plr.Name then
                                        for i, v in pairs(Workspace[Plr.Name]:GetChildren()) do
                                        if v:IsA("BasePart") then
                                        v.CanCollide = false
                                        end end end end
                                    else
                                        Stepped:Disconnect()
                                    end
                                end)
                            elseif benoclip == false then
                                Clipon = false
                            
                            end
                        end    
                    })

                        Tab10:AddButton({
                            Name = "ANTI AFK (rejoin to disable)",
                            Callback = function()

                                for _, v in next, getconnections(game:GetService("Players").LocalPlayer.Idled) do
                                    v:Disable()
                                end 
                                    OrionLib:MakeNotification({
                                        Name = "ANTI AFK HAS BEEN ENABLED",
                                        Content = "if you want to disable rejoin :)",
                                        Image = "rbxassetid://4483345998",
                                        Time = 5
                                    })
                                    
                                   
                            end 
                    })
            



                    Tab10:AddSlider({
                        Name = "Walk Speed",
                        Min = 0,
                        Max = 500,
                        Default = 25,
                        Color = Color3.fromRGB(255,255,255),
                        Increment = 1,
                        ValueName = "Speed",
                        Callback = function(Value)
                            getgenv().bool = Value            

                        end    
                    })

                    Tab10:AddToggle({
                        Name = "Walk Speed On/Off",
                        Default = false,
                        Callback = function(Value)
                            g = Value

                            while g do task.wait()
                                game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = bool
                            end

                        end    
                    })

                    Tab10:AddSlider({
                        Name = "Jump Power",
                        Min = 0,
                        Max = 500,
                        Default = 25,
                        Color = Color3.fromRGB(255,255,255),
                        Increment = 1,
                        ValueName = "Speed",
                        Callback = function(Value)
                            getgenv().bool2 = Value            

                        end    
                    })

                    Tab10:AddToggle({
                        Name = "Jump Power On/Off",
                        Default = false,
                        Callback = function(Value)
                            g = Value

                            while g do task.wait()
                                game.Players.LocalPlayer.Character.Humanoid.JumpPower = bool2
                            end

                        end    
                    })

                    Tab10:AddButton({
                        Name = "Discord Server",
                        Callback = function()
                            setclipboard("https://discord.gg/NJ3u2pVjcs")
                            toclipboard("https://discord.gg/NJ3u2pVjcs")

                                OrionLib:MakeNotification({
                                    Name = "Link Copied!",
                                    Content = "Paste it in Browser",
                                    Image = "rbxassetid://4483345998",
                                    Time = 5
                                })
                                
                               
                        end 
                })

                Tab10:AddToggle({
                    Name = "Inf Stamina",
                    Default = false,
                    Callback = function(Value)
                        v = Value
                    local stamina = game:GetService("ReplicatedStorage").Events["Stamina/RemoteEvent"]

                    if v then  

                        stamina:Destroy()
                        wait()
                        
                    end
                end    
                })
