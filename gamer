                  function AUTO1Quest()
                  
                          local lp = game.Players.LocalPlayer
                  
                          local function getNPC()
                              for i,v in pairs(game:GetService("Workspace").Scriptable.Mobs:GetChildren()) do
                                  if v.Name == "Thug1" then
                                      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Head.CFrame
                                      
                                      
                                  end     
                              end
                              wait(0.1)
                              for i,v in pairs(game:GetService("Workspace").Scriptable.Mobs:GetChildren()) do
                                  if v.Name == "Thug2" then
                                      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Head.CFrame
                                      
                                      
                                  end     
                              end
                          end
                  
                          while getgenv().autoQuest1T == true do 
                          task.wait()
                              pcall(function()
                                  moveto(getNPC().HumanoidRootPart.CFrame + Vector3.new(50,100,300) ,100)
                                  wait(0.1)
                              end)
