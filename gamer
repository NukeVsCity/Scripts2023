                                      rootPart.CFrame = game:GetService("Workspace").Scriptable.Crates.common.WorldPivot
                                      end
                                      game:GetService("VirtualInputManager"):SendKeyEvent(true,Enum.KeyCode.E,false,game)
                                  end
                              end)
                          end
                      
