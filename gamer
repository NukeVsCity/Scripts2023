            Name = "Farm Distance",
            Min = 0,
            Max = 80,
            Default = 9,
            Color = Color3.fromRGB(255,255,255),
            Increment = 1,
            ValueName = "Distance",
            Callback = function(Value)
                getgenv().dista = Value
            end    
        })


        

        Tab5:AddToggle({
            Name = "Start Auto Farm",
            Default = false,
            Callback = function(Value)
                t = Value
                while t do task.wait()
                    
                            pcall(function()
                                moveto(getNPCCC().HumanoidRootPart.CFrame + Vector3.new(0,0,0) ,100)               
                                wait()
                    end)
                end
            end    
        }) 

