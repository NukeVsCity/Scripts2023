--Functions
                        
                      

                      local NPCS = {}
                            for i, v in pairs(game:GetService("Workspace").Scriptable.Mobs:GetDescendants()) do
                                if v:IsA "Model" and v:FindFirstChild("HumanoidRootPart") then
                                    if not table.find(NPCS, tostring(v)) then
                                        table.insert(NPCS, tostring(v))
                                    end
                                end
                            end

                      
                      
                      
                      local BossT = {}
                  
                          for i, v in pairs(game:GetService("Workspace").Scriptable.Bosses:GetDescendants()) do
                              if v:IsA "Model" and v:FindFirstChild("HumanoidRootPart") then
                                  if not table.find(BossT, tostring(v)) then
                                  table.insert(BossT, tostring(v))
                                  end
                              end
                          end
                  
                               
                          
                          
                          
                  
                          local TPDimension = {}
                  
                                  for i, v in pairs(game:GetService("Workspace").Scriptable.TrainingsAreas:GetDescendants()) do
                                      if v:IsA "Part" then
                                          if not table.find(TPDimension, tostring(v)) then
                                          table.insert(TPDimension, tostring(v))
                                           end
                                      end
                                  end


                                  local QuestNPCS = {}
                  
                                  for i, v in pairs(workspace.Scriptable.NPC.Quests:GetDescendants()) do
                                      if v:IsA "Model" then
                                          if not table.find(QuestNPCS, tostring(v)) then
                                          table.insert(QuestNPCS, tostring(v))
                                           end
                                      end
                                  end
                  
                       
                      local function getNPCCC()
                  
                          for i, v in pairs(game:GetService("Workspace").Scriptable.Mobs:GetChildren()) do
                              if v:IsA "Model" and v.Name == amobs then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Head.CFrame + Vector3.new(0, dista, 0)
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.lookAt(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, v.Head.Position)
                  
                                  end
                      
                          end
                      
                      end
                      
                      local function DoABoss()                        
                       
                        
                            for i, v in pairs(game:GetService("Workspace").Scriptable.Bosses:GetChildren()) do
                                if v:IsA "Model" and v.Name == aBoss then
                                      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Head.CFrame + Vector3.new(0,dista2,0) 
                                      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.lookAt(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, v.Head.Position)
                    
                                end
                        
                            end
                        end
                        
                        

                     


                      local function BossGet()
                  
                          for i, v in pairs(game:GetService("Workspace").Scriptable.Bosses:GetChildren()) do
                              if v:IsA "Model" and v.Name == bosss then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Head.CFrame + Vector3.new(0,dista2,0) 
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.lookAt(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, v.Head.Position)
                  
                              end
                      
                          end
                      
                      end
                  
                  
                      local function TPdim()
                  
                          for i, v in pairs(game:GetService("Workspace").Scriptable.TrainingsAreas:GetChildren()) do
                              if v:IsA "Part" and v.Name == tpareas then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame + Vector3.new(0,2,0) 
                  
                              end
                      
                          end
                      
                      end
                  
                      local function getFRUIT()
                          
                          for _,v in pairs(game:GetService("Workspace"):GetChildren()) do
                              if v:IsA "Model" and v.Name == "chop" or v.Name == "gomu" or v.Name == "mera" or v.Name == "ice" or v.Name == "gura" or v.Name == "magma" or v.Name == "buddha" or v.Name == "light" then
                              OrionLib:MakeNotification({
                                Name = "Fruit Found!",
                                Content = "Teleporting in few secs",
                                Image = "rbxassetid://4483345998",
                                Time = 5
                            }) 
                            wait(1)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Mesh.CFrame

                                
                              end
                          end
                      end
                  
                  
                  
                  local function NPCSQuestss()
                    for i, v in pairs(workspace.Scriptable.NPC.Quests:GetDescendants()) do
                        if v:IsA "Model" and v.Name == npcsquest then
                              game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame + Vector3.new(0,2,0) 
            
                        end
                
                    end

                  end
                  
                  
                  
                          function AutoStr()
                              spawn(function()
                                  while getgenv().autoStren == true do
                                      local args = {
                                          [1] = "TrainStat",
                                          [2] = "Strength"}
                      
                                      game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Stats/RemoteFunction"):InvokeServer(unpack(args))
                                      wait()    
                                  end
                              end)
                          end
                      
                  
                  
